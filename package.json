{
  "name": "aisdk-storefront",
  "version": "0.2.0",
  "type": "module",
  "description": "Production-ready e-commerce platform demonstrating Next.js 15 experimental features (PPR, Server Components, modern caching) with Convex real-time database, Better Auth, Polar payments, and AI virtual try-on",
  "author": "Ray <hi@rmncldyo.com> (https://github.com/RMNCLDYO)",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/RMNCLDYO/aisdk-storefront"
  },
  "bugs": {
    "url": "https://github.com/RMNCLDYO/aisdk-storefront/issues"
  },
  "homepage": "https://github.com/RMNCLDYO/aisdk-storefront#readme",
  "scripts": {
    "dev": "npm-run-all --parallel dev:frontend dev:backend",
    "dev:backend": "convex dev --typecheck-components --live-component-sources",
    "dev:frontend": "next dev --experimental-https",
    "generate": "convex dev --once --typecheck-components",
    "build": "next build",
    "start": "next start",
    "lint": "biome format --write && tsc -p convex",
    "clean": "rm -rf .next && rm -rf node_modules && rm -rf dist && npm install",
    "db:reset": "npx convex run factoryReset:factoryReset",
    "polar:seed": "npx tsx scripts/seedAll.ts",
    "polar:seed-products": "npx tsx scripts/seedProducts.ts",
    "polar:seed-subscriptions": "npx tsx scripts/seedSubscriptions.ts",
    "polar:verify-seed": "npx tsx scripts/verifySeeding.ts"
  },
  "dependencies": {
    "@ai-sdk/google": "^2.0.17",
    "@convex-dev/better-auth": "^0.8.9",
    "@convex-dev/polar": "^0.6.4",
    "@convex-dev/resend": "^0.1.13",
    "@polar-sh/sdk": "^0.35.4",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@react-email/components": "0.5.6",
    "ai": "^5.0.60",
    "better-auth": "1.3.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "convex": "^1.27.4",
    "convex-helpers": "^0.1.104",
    "lucide-react": "^0.545.0",
    "next": "^15.6.0-canary.34",
    "next-themes": "^0.4.6",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-qr-code": "^2.0.18",
    "server-only": "^0.0.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.5",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^24",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv": "^17.2.3",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^4",
    "tsx": "^4.20.6",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5"
  },
  "overrides": {
    "better-auth": "1.3.8"
  }
}
