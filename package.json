{
  "name": "aisdk-storefront",
  "version": "0.9.0",
  "type": "module",
  "description": "Production-ready e-commerce platform demonstrating Next.js 16 features (Cache Components, PPR, Server Components, modern caching) with Convex real-time database, Better Auth, Polar payments, and AI virtual try-on",
  "author": "Ray <hi@rmncldyo.com> (https://github.com/RMNCLDYO)",
  "license": "MIT",
  "private": false,
  "engines": {
    "node": ">=20.9.0",
    "npm": ">=10.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/RMNCLDYO/aisdk-storefront"
  },
  "bugs": {
    "url": "https://github.com/RMNCLDYO/aisdk-storefront/issues"
  },
  "homepage": "https://github.com/RMNCLDYO/aisdk-storefront#readme",
  "scripts": {
    "dev": "npm-run-all --parallel dev:frontend dev:backend",
    "dev:backend": "convex dev --typecheck-components --live-component-sources",
    "dev:frontend": "next dev --experimental-https",
    "generate": "convex dev --once --typecheck-components",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . && tsc -p convex",
    "format": "eslint . --fix",
    "clean": "rm -rf .next && rm -rf node_modules && rm -rf package-lock.json && rm -rf dist && npm install",
    "db:reset": "npx convex run utils/clearDatabase:clearDatabase",
    "polar:seed": "npx tsx scripts/seedAll.ts",
    "polar:seed-products": "npx tsx scripts/seedProducts.ts",
    "polar:seed-subscriptions": "npx tsx scripts/seedSubscriptions.ts"
  },
  "dependencies": {
    "@ai-sdk/google": "^2.0.23",
    "@convex-dev/better-auth": "^0.9.6",
    "@convex-dev/polar": "^0.6.4",
    "@convex-dev/resend": "^0.1.13",
    "@polar-sh/sdk": "^0.38.1",
    "@radix-ui/react-aspect-ratio": "^1.1.7",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@react-email/components": "0.5.7",
    "ai": "^5.0.76",
    "babel-plugin-react-compiler": "1.0.0",
    "better-auth": "1.3.27",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "convex": "^1.28.0",
    "convex-helpers": "^0.1.104",
    "lucide-react": "^0.546.0",
    "next": "^16.0.0",
    "next-themes": "^0.4.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-qr-code": "^2.0.18",
    "server-only": "^0.0.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "vaul": "^1.1.2",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^24",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "8.46.2",
    "dotenv": "^17.2.3",
    "eslint": "9.38.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react-hooks": "7.0.0",
    "eslint-plugin-security": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^4",
    "tsx": "^4.20.6",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5"
  },
  "overrides": {
    "better-auth": "1.3.27"
  }
}
